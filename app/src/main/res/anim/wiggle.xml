<?xml version="1.0" encoding="utf-8"?>
<!-- Wiggle animation for attention -->
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <rotate
        android:duration="50"
        android:fromDegrees="0"
        android:toDegrees="5"
        android:pivotX="50%"
        android:pivotY="50%"
        android:repeatCount="5"
        android:repeatMode="reverse" />
</set>
