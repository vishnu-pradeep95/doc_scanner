<?xml version="1.0" encoding="utf-8"?>
<!-- ic_redo.xml - Time-traveling Arrow Mascot (going forward again!) -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">
    
    <!-- Circular time swirl background -->
    <path
        android:fillColor="#4ECDC4"
        android:strokeColor="#2C3E50"
        android:strokeWidth="1.5"
        android:pathData="M12,3 A9,9 0,1,1 12,21 A9,9 0,1,1 12,3" />
    
    <!-- Inner circle -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M12,6 A6,6 0,1,1 12,18 A6,6 0,1,1 12,6" />
    
    <!-- Curved arrow going forward -->
    <path
        android:strokeColor="#FFE66D"
        android:strokeWidth="2.5"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:fillColor="@android:color/transparent"
        android:pathData="M9,9 C9,9 11,7 14,8 C17,9 17,12 17,12" />
    
    <!-- Arrow head pointing right (going forward) -->
    <path
        android:fillColor="#FFE66D"
        android:pathData="M17,8 L19,12 L15,12 Z" />
    
    <!-- Googly eyes on the arrow curve -->
    <path
        android:fillColor="#FFFFFF"
        android:strokeColor="#2C3E50"
        android:strokeWidth="0.6"
        android:pathData="M8.6,8 A1.2,1.2 0,1,1 11,8 A1.2,1.2 0,1,1 8.6,8" />
    <path
        android:fillColor="#2C3E50"
        android:pathData="M9.7,8.2 A0.4,0.4 0,1,1 10.5,8.2 A0.4,0.4 0,1,1 9.7,8.2" />
    
    <!-- Little time sparkles -->
    <path
        android:fillColor="#FF6B6B"
        android:pathData="M7,14 L7.2,14.5 L7.7,14.6 L7.2,14.7 L7,15.2 L6.8,14.7 L6.3,14.6 L6.8,14.5 Z" />

</vector>
