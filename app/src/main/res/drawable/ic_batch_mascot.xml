<?xml version="1.0" encoding="utf-8"?>
<!-- Batch mode icon - Multiple documents with happy faces stacked together -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">
    
    <!-- Back document (shadow) -->
    <path
        android:pathData="M6,3 L16,3 L18,5 L18,12 L6,12 Z"
        android:fillColor="#4ECDC4"
        android:fillAlpha="0.3"/>
    
    <!-- Middle document -->
    <path
        android:pathData="M5,6 L15,6 L17,8 L17,16 L5,16 Z"
        android:fillColor="#FFE66D"
        android:strokeWidth="2"
        android:strokeColor="#2C3E50"/>
    
    <!-- Front document with happy face -->
    <path
        android:pathData="M4,10 L14,10 L16,12 L16,20 L4,20 Z"
        android:fillColor="#4ECDC4"
        android:strokeWidth="2.5"
        android:strokeColor="#2C3E50"/>
    
    <!-- Googly eyes on front document -->
    <path
        android:pathData="M 8,14 A 1.2,1.2 0 1,1 8,13.999 Z"
        android:fillColor="#FFFFFF"
        android:strokeWidth="1"
        android:strokeColor="#2C3E50"/>
    <path
        android:pathData="M 8.3,14 A 0.6,0.6 0 1,1 8.3,13.999 Z"
        android:fillColor="#2C3E50"/>
    
    <path
        android:pathData="M 12,14 A 1.2,1.2 0 1,1 12,13.999 Z"
        android:fillColor="#FFFFFF"
        android:strokeWidth="1"
        android:strokeColor="#2C3E50"/>
    <path
        android:pathData="M 12.3,14 A 0.6,0.6 0 1,1 12.3,13.999 Z"
        android:fillColor="#2C3E50"/>
    
    <!-- Happy smile -->
    <path
        android:pathData="M7,17 Q10,18.5 13,17"
        android:strokeWidth="1.5"
        android:strokeColor="#2C3E50"
        android:strokeLineCap="round"
        android:fillColor="@android:color/transparent"/>
    
    <!-- Plus badge -->
    <group
        android:translateX="15"
        android:translateY="4">
        <path
            android:pathData="M 4,4 A 4,4 0 1,1 4,3.999 Z"
            android:fillColor="#FF6B6B"/>
        <path
            android:pathData="M2,4 L6,4 M4,2 L4,6"
            android:strokeWidth="1.5"
            android:strokeColor="#FFFFFF"
            android:strokeLineCap="round"/>
    </group>
    
</vector>
