<?xml version="1.0" encoding="utf-8"?>
<!-- History/Clock icon with happy face -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">
    
    <!-- Clock circle -->
    <path
        android:pathData="M 12,12 A 9,9 0 1,1 12,11.999 Z"
        android:fillColor="#FFE66D"
        android:strokeWidth="2"
        android:strokeColor="#2C3E50"/>
    
    <!-- Clock hands -->
    <path
        android:pathData="M12,7 L12,12 L16,14"
        android:strokeWidth="2"
        android:strokeColor="#2C3E50"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:fillColor="@android:color/transparent"/>
    
    <!-- Happy eyes -->
    <path
        android:pathData="M 9,10 A 1,1 0 1,1 9,9.999 Z"
        android:fillColor="#FFFFFF"
        android:strokeWidth="0.8"
        android:strokeColor="#2C3E50"/>
    <path
        android:pathData="M 9.2,10 A 0.5,0.5 0 1,1 9.2,9.999 Z"
        android:fillColor="#2C3E50"/>
    
    <path
        android:pathData="M 15,10 A 1,1 0 1,1 15,9.999 Z"
        android:fillColor="#FFFFFF"
        android:strokeWidth="0.8"
        android:strokeColor="#2C3E50"/>
    <path
        android:pathData="M 15.2,10 A 0.5,0.5 0 1,1 15.2,9.999 Z"
        android:fillColor="#2C3E50"/>
    
    <!-- Smile -->
    <path
        android:pathData="M9,16 Q12,18 15,16"
        android:strokeWidth="1.5"
        android:strokeColor="#2C3E50"
        android:strokeLineCap="round"
        android:fillColor="@android:color/transparent"/>
    
</vector>
