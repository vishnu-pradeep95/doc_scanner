<?xml version="1.0" encoding="utf-8"?>
<!-- Menu/reorder icon with stacked lines mascot -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">
    
    <!-- Three horizontal lines -->
    <path
        android:pathData="M4,7 L20,7"
        android:strokeWidth="2.5"
        android:strokeColor="#4ECDC4"
        android:strokeLineCap="round"/>
    
    <path
        android:pathData="M4,12 L20,12"
        android:strokeWidth="2.5"
        android:strokeColor="#FFE66D"
        android:strokeLineCap="round"/>
    
    <path
        android:pathData="M4,17 L20,17"
        android:strokeWidth="2.5"
        android:strokeColor="#FF6B6B"
        android:strokeLineCap="round"/>
    
    <!-- Dots at the start of each line -->
    <path
        android:pathData="M 3,7 A 1.5,1.5 0 1,1 3,6.999 Z"
        android:fillColor="#4ECDC4"
        android:strokeWidth="1"
        android:strokeColor="#2C3E50"/>
    
    <path
        android:pathData="M 3,12 A 1.5,1.5 0 1,1 3,11.999 Z"
        android:fillColor="#FFE66D"
        android:strokeWidth="1"
        android:strokeColor="#2C3E50"/>
    
    <path
        android:pathData="M 3,17 A 1.5,1.5 0 1,1 3,16.999 Z"
        android:fillColor="#FF6B6B"
        android:strokeWidth="1"
        android:strokeColor="#2C3E50"/>
    
</vector>
