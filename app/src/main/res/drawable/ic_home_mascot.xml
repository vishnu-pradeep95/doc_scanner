<?xml version="1.0" encoding="utf-8"?>
<!-- Home icon with happy house mascot for toolbar -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">
    
    <!-- House body -->
    <path
        android:pathData="M4,10 L12,3 L20,10 L20,20 L4,20 Z"
        android:fillColor="#4ECDC4"
        android:strokeWidth="2"
        android:strokeColor="#2C3E50"/>
    
    <!-- Roof -->
    <path
        android:pathData="M2,10 L12,2 L22,10"
        android:strokeWidth="2.5"
        android:strokeColor="#FF6B6B"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:fillColor="@android:color/transparent"/>
    
    <!-- Door -->
    <path
        android:pathData="M10,14 L14,14 L14,20 L10,20 Z"
        android:fillColor="#FFE66D"
        android:strokeWidth="1.5"
        android:strokeColor="#2C3E50"/>
    
    <!-- Window with happy face -->
    <path
        android:pathData="M6,12 L9,12 L9,16 L6,16 Z"
        android:fillColor="#FFFFFF"
        android:strokeWidth="1.2"
        android:strokeColor="#2C3E50"/>
    
    <!-- Window eyes -->
    <path
        android:pathData="M 7,13.5 A 0.4,0.4 0 1,1 7,13.499 Z"
        android:fillColor="#2C3E50"/>
    <path
        android:pathData="M 8,13.5 A 0.4,0.4 0 1,1 8,13.499 Z"
        android:fillColor="#2C3E50"/>
    
    <!-- Window smile -->
    <path
        android:pathData="M6.5,14.8 Q7.5,15.3 8.5,14.8"
        android:strokeWidth="0.8"
        android:strokeColor="#2C3E50"
        android:strokeLineCap="round"
        android:fillColor="@android:color/transparent"/>
    
    <!-- Chimney -->
    <path
        android:pathData="M16,5 L18,5 L18,8 L16,8 Z"
        android:fillColor="#FF6B6B"
        android:strokeWidth="1.2"
        android:strokeColor="#2C3E50"/>
    
</vector>
